	.data
day:		.word 0
month:		.word 0
year:		.word 0
weekday:	.word 0
printDay:	.string "Enter a day.\n"
printMonth:	.string "Enter a month.\n"
printYear:	.string "Enter a year.\n"
scanFmt:	.string "%s"


	.text
	.type main, %function
	.global main

main:
	stp x29, x30, [sp, #-16]!






end:
	ldp x29, x30, [sp], #16
	mov x0, #0
	ret

	.size main, (.-main)
