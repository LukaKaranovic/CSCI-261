 *
 *  Writes a multiplication table of base number a from 1 to n.
 */

	.data
a:	.word 0
n:	.word 0
inputA:	.string "Enter a base number for your table.\n"
inputN:	.string "Enter the number you want your multiplication table up to.\n"
scanFmt:	.string "%d"
outputMsg:	.string "%d X %d = %d\n"


 	.text
	.type main, %function
	.global main

main:
	stp x29, x30, [sp, #-16]!


	// Input a
	adr x0, inputA
	bl printf

	adr x0, scanFmt
	adr x1, a
	bl scanf


	// Input n
	adr x0, inputN
	bl printf

	adr x0, scanFmt
	adr x1, n
	bl scanf

	mov x18, #1 // int i = 1
	ldr x19, n  // int n
	ldr x20, a  // int a

loop:
	cmp x19, x18
	beq end
	adr x0, outputMsg
	ldr x1, x20
	ldr x2, x18
	mull x3, x1, x2
	bl printf

end:

	ldp x29, x30, [sp], #16
	mov x0, 0
	ret

	.size main, (.-main)
